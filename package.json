    "devDependencies": {
        "standard": "*",
        "babel": "^6.3.13",
        "babel-core": "^6.1.18",
        "babel-loader": "^6.1.0",
        "babel-plugin-add-module-exports": "^0.1.2",
        "babel-preset-es2015": "^6.3.13",
        "mocha": "^2.3.4",
        "chai": "^3.4.1",
        "webpack": "^1.12.9",
        "yargs": "^3.32.0"
